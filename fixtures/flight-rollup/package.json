{
  "name": "flight-rollup",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "cp -r ../../build/oss-experimental/* ./node_modules/",
    "prebuild": "rm -rf build-* && cp -r ../../build/oss-experimental/* ./node_modules/",
    "postbuild": "prettier --no-config --ignore-path none --no-error-on-unmatched-pattern --write build-rsc/**/*.{js,css,html,json}",
    "dev": "concurrently \"npm run dev:rsc\" \"npm run dev:ssr\"",
    "dev:ssr": "NODE_ENV=development BUILD_PATH=dist node --inspect=127.0.0.1:9230 server/ssr-server.js",
    "dev:rsc": "NODE_ENV=development BUILD_PATH=dist nodemon --watch src --watch dist -- --enable-source-maps --conditions=react-server --inspect=127.0.0.1:9229 server/rsc-server.js",
    "start": "node scripts/build.js && concurrently \"npm run start:rsc\" \"npm run start:ssr\"",
    "start:ssr": "NODE_ENV=production node build-ssr/ssr-server.js",
    "start:rsc": "NODE_ENV=production node --conditions=react-server build-rsc/rsc-server.js",
    "build": "node scripts/build.js"
  },
  "dependencies": {    
    "body-parser": "^1.20.3",
    "busboy": "^1.6.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "react": "experimental",
    "react-dom": "experimental"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.25.7",
    "@babel/preset-react": "^7.22.5",
    "@babel/runtime": "^7.25.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "babel-preset-react-app": "^10.0.1",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "prettier": "^3.4.2",
    "react-server-dom-webpack": "experimental",
    "rollup": "^4.24.0",
    "rollup-plugin-node-externals": "^7.1.3"
  },
  "devEngines": {
    "node": "20.x || 22.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "volta": {
    "node": "22.13.1"
  }
}
